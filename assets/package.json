  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-eslint": "^10.0.2",
    "babel-loader": "^7.1.3",
    "babel-preset-env": "^1.6.1",
    "copy-webpack-plugin": "^4.5.0",
